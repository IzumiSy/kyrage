{
  "name": "@izumisy/kyrage",
  "private": false,
  "type": "module",
  "version": "1.5.3",
  "description": "A minimal, schema-based declarative migration tool for Node.js ecosystem",
  "author": "Seiya Izumi (https://github.com/IzumiSy)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IzumiSy/kyrage.git"
  },
  "homepage": "https://github.com/IzumiSy/kyrage",
  "keywords": [
    "migration",
    "database",
    "schema",
    "kysely"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": {
    "kyrage": "./dist/main.mjs"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsdown --watch",
    "build": "tsdown",
    "type-check": "tsc",
    "test": "vitest run"
  },
  "dependencies": {
    "@testcontainers/cockroachdb": "^11.7.1",
    "@testcontainers/postgresql": "^11.7.1",
    "better-sqlite3": "^12.4.1",
    "c12": "^3.3.3",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "graph-data-structure": "^4.5.0",
    "kysely": "^0.28.8",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "ramda": "^0.32.0",
    "sql-formatter": "^15.6.10",
    "testcontainers": "^11.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25",
    "@types/pg": "^8.15.5",
    "@types/ramda": "^0.31.1",
    "memfs": "^4.49.0",
    "prettier": "^3",
    "tsdown": "^0.18.4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
